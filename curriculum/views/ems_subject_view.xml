<odoo>
    <data>
        <!-- Inherit Patient Form View -->
        <record id="view_ems_subject" model="ir.ui.view">
            <field name="model">ems.subject</field>
            <field name="inherit_id" ref="ems_timetable.ems_subject_view_form" />
            <field name="arch" type="xml">
                 <xpath expr="//field[@name='is_practical']" position="after">
                    <field name="book_id" />
                 </xpath>
            </field>
        </record>
    </data>
</odoo>